{
    "manifest_version": 2,

    "name": "Fill Score",
    "description": "This extension fill score automaticly.",
    "version": "1.0",

    "icons":
    {
        "19": "data/icons/19.png",
        "38": "data/icons/38.png",
        "64": "data/icons/64.png",
        "128": "data/icons/128.png"
    },
    "background":
    {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "page_action":
    {
        "default_icon": "data/icons/19.png",
        "default_title": "Fill Score",
        "default_popup": "popup.html"
    },    
    "permissions": ["storage", "tabs", "notifications", "http://211.81.249.99/*"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": 
    [
        {
            "matches": ["http://211.81.249.99/js_main.aspx?*"],
            "js": ["js/jquery-2.0.0.min.js", "js/autofill.js"],
            "run_at": "document_idle"
        }
    ]
}
